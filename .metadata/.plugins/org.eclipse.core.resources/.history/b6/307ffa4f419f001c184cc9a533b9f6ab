
public class TestaGetESet {
	
	public static void main(String[] args) {
		
		Conta conta = new Conta();
		conta.setNumero(420);
		System.out.println("============================================");
		System.out.println("Nova conta aberta Nº " + conta.getNumero());
		
		Cliente cliente = new Cliente();
		cliente.setNome("janderson paixao");
		System.out.println("nome do Cliente: " + cliente.getNome());
		conta.setTitular(cliente);
		System.out.println("Titular da conta: " + conta.getTitular());
		cliente.setCpf("042004200-420");
		System.out.println("CPF: " + cliente.getCpf());
		cliente.setProfissao("Programador");
		System.out.println("Profissão: " + cliente.getProfissao());
		System.out.println("============================================");
		System.out.println("============================================");
		System.out.println("Novo deposito efetuado: R$8.000.00");
		
		
		conta.deposita(8000);
		System.out.println("Saldo atual da conta: R$" + conta.getSaldo());
		System.out.println("============================================");
		conta.setAgencia(860);
		System.out.println("Agência: " + conta.getAgencia());
		
		
		
	}
}
